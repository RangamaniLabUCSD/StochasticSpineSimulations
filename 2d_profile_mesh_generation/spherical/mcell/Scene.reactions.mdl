  VSCC_0' <-> VSCC_1' [>"python_scripts/vscc_1_forward_scaled.txt", <"python_scripts/vscc_1_reverse_scaled.txt"]
  VSCC_1' <-> VSCC_2' [>"python_scripts/vscc_2_forward_scaled.txt", <"python_scripts/vscc_2_reverse_scaled.txt"]
